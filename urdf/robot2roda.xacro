<?xml version="1.0" ?>
<robot name="robot2roda" xmlns:xacro="https://www.ros.org/wiki/xacro" >
 
  <!-- include the xacro files-->  
  <xacro:include filename="$(find tugas_kelompok)/urdf/materials.xacro" />
  <xacro:include filename="$(find tugas_kelompok)/urdf/gazebo.xacro" />
  <xacro:include filename="$(find tugas_kelompok)/urdf/macro.xacro" />
 
  <!-- Chasis defined here -->
  <link name="link_chassis">
    <pose>0 0 0.1 0 0 0</pose>
    <inertial>
      <mass value="5"/>
      <origin rpy="0 0 0" xyz="0 0 0.1"/>
      <inertia ixx="0.0395416666667" ixy="0" ixz="0" iyy="0.106208333333" iyz="0" izz="0.106208333333"/>
    </inertial>
 
    <collision name="collision_chassis">
      <geometry>
        <box size="0.5 0.3 0.07"/>
      </geometry>
    </collision>
 
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.5 0.3 0.07"/>
      </geometry>
      <material name="blue"/>
    </visual>
 
    <!-- caster front -->
<!-- <collision name="caster_front_collision">
      <origin rpy=" 0 0 0" xyz="0.35 0 -0.05"/>
      <geometry>
        <sphere radius="0.05"/>
